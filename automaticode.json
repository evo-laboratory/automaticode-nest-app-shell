{
  "Config": {
    "SchemaVersion": 1,
    "SchemaFolder": "automaticode/schemas",
    "GDKOutPutFolder": "src/gdk-modules",
    "User": {
      "ExtraSchemaJsonName": "user-extra"
    }
  },
  "Auth": {
    "Provider": "Firebase",
    "Roles": ["general", "super", "admin", "staff", "client", "owner"],
    "AdminRole": "super",
    "DefaultRole": "general",
    "JWT": {
      "CustomTokenUserPayloadProperties": [
        "roles"
      ]
    }
  },
  "Mail": {
    "Provider": "Sendgrid"
  },
  "Logger": {
    "Provider": "Local"
  },
  "Database": {
    "Provider": "MongoDB",
    "ORMEngine": "mongoose"
  }
}